        init(z[$Z_STEP * i], i);
    }

    init(z[$Z_STEP * N - $Z_STEP * 4], $Z_STEP * N - 8);
    init(z[$Z_STEP * N - $Z_STEP * 3], $Z_STEP * N - 6);
    init(z[$Z_STEP * N - $Z_STEP * 2], $Z_STEP * N - 3);
    init(z[$Z_STEP * N - $Z_STEP * 1], $Z_STEP * N - 2);
    init(z[$Z_STEP * N], $Z_STEP * N);

    // y
    // for j = 1..3 {
    //     print(y[j]);
    // }
    // 
    // init(y[1], 1);
    // init(y[2], 2);
    // init(y[3], 3);

    // x
    for i = 0..20 {
        print(x[i]);
    }

    init(x[0], 0);

    for j = 1..9 {
        init(x[j], j);
    }

    init(x[10], 10);
    // init(x[11], 11);

    for k = 12..14 {
        init(x[k], k);
    }

    for k = 15..18 {
        init(x[k], k);
    }

    init(x[19], 19);
    init(x[20], 20);
}
